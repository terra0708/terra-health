{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@core": [
                "src/core"
            ],
            "@core/*": [
                "src/core/*"
            ],
            "@modules": [
                "src/modules"
            ],
            "@modules/*": [
                "src/modules/*"
            ],
            "@common": [
                "src/common"
            ],
            "@common/*": [
                "src/common/*"
            ],
            "@views": [
                "src/views"
            ],
            "@views/*": [
                "src/views/*"
            ],
            "@actions": [
                "src/actions"
            ],
            "@actions/*": [
                "src/actions/*"
            ],
            "@assets": [
                "src/assets"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@mocks": [
                "src/mocks"
            ],
            "@mocks/*": [
                "src/mocks/*"
            ],
            "@app": [
                "src/app"
            ],
            "@app/*": [
                "src/app/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}